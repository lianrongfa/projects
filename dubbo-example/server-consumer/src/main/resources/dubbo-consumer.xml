<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
           xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans.xsd
           http://code.alibabatech.com/schema/dubbo
           http://code.alibabatech.com/schema/dubbo/dubbo.xsd">
    <!--服务名称-->
    <dubbo:application name="server-consumer" owner="lianrf"/>

    <!--注册中心 如果不需要注册中心 address为:N/A  zookeeper://120.77.243.179:2181-->
    <dubbo:registry id="zk" address="zookeeper://120.77.243.179:2181"/>

    <!--无注册中心时需指定url-->
    <dubbo:reference id="userServiceImpl" interface="cn.lianrf.IUserService" url="dubbo://192.168.1.109:20880/cn.lianrf.IUserService"/>

</beans>